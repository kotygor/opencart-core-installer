{
    "name": "kotygor/opencart-core-installer",
    "description": "Composer installer for Opencart",
    "type": "composer-plugin",
    "authors": [
        {
            "name": "Etki",
            "email": "etki@etki.name",
            "homepage": "http://etki.name",
            "role": "Developer"
        }],
    "support": {
        "issues": "https://github.com/etki/opencart-core-installer/issues",
        "source": "https://github.com/etki/opencart-core-installer.git"
    },
    "license": "MIT",
    "version": "0.1.3",
    "keywords": ["installer", "opencart"],
    "autoload": {
        "psr-4": {"Etki\\Composer\\Installers\\Opencart\\": "src/"}
    },
    "extra": {
        "class": "Etki\\Composer\\Installers\\Opencart\\Plugin"
    },
    "require": {
        "php": ">=7.0",
        "symfony/console": "~5",
        "symfony/filesystem": "~5",
        "composer-plugin-api": "~1.1"
    },
    "require-dev": {
        "composer/composer": "~1.0@dev",
        "codeception/codeception": "4.1.x-dev"
    }
}
